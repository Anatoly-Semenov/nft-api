  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:up": "yarn run typeorm migration:run --config src/common/config/typeorm.config.ts",
    "migration:down": "yarn run typeorm migration:revert --config src/common/config/typeorm.config.ts ",
    "migration:create": "yarn run typeorm migration:create --config src/common/config/typeorm.config.ts -n",
    "migration:generate": "yarn run typeorm migration:generate --config src/common/config/typeorm.config.ts -n",
    "create:telegram:session": "ts-node ./src/createTelegramClientSession.ts"
  },
